SCENE main {
	/* Instantiate and array */
	array<int> my_array;

	/* Push the four numbers into the array */
	int count = (10, 20, "30", 40 => my_array);

	/* Print result */
	echo "Pushed $count values to the array\n";

	int i = "-1";
	while (++i < @my_array) {
		echo "my_array[$i] is " . my_array[i] . "\n";
	}
}

